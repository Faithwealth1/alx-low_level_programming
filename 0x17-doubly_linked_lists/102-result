#include <stdio.h>

int is_palindrome(int n) {
    int rev = 0, tmp = n;
    while (tmp > 0) {
        rev = rev * 10 + tmp % 10;
        tmp /= 10;
    }
    return (n == rev);
}

int main() {
    int max_palindrome = 0;
    int i, j;
    for (i = 100; i <= 999; i++) {
        for (j = i; j <= 999; j++) {
            int product = i * j;
            if (is_palindrome(product) && product > max_palindrome) {
                max_palindrome = product;
            }
        }
    }
    FILE *f = fopen("102-result", "w");
    fprintf(f, "%d", max_palindrome);
    fclose(f);
    return 0;
}
